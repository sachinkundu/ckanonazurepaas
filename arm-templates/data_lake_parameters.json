{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "location": {
          "value": "westeurope"
      },
      "storageAccountName": {
          "value": "ckanpublicstorage"
      },
      "storageContainerNames": {
          "value": [
              "data",
              "webassets"
          ]
      },
      "accountType": {
          "value": "Standard_LRS"
      },
      "kind": {
          "value": "StorageV2"
      },
      "accessTier": {
          "value": "Hot"
      },
      "supportsHttpsTrafficOnly": {
          "value": true
      },
      "allowBlobPublicAccess": {
          "value": true
      },
      "networkAclsBypass": {
          "value": "AzureServices"
      },
      "networkAclsDefaultAction": {
          "value": "Allow"
      },
      "isHnsEnabled": {
          "value": false
      }
    }
  }
  